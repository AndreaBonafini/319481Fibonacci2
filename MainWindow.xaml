<Window x:Class="Fibonacci_sequence_drawing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fibonacci_sequence_drawing"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
   
    <Canvas>
        <TextBox x:Name="txtNumber" Width="40"></TextBox>
        <Button x:Name="btnNumber" Click="btnNumber_Click" Content="fib" RenderTransformOrigin="3.098,0.823" Width="36" Canvas.Left="40" Height="18"></Button>
        <Canvas x:Name="canvas"></Canvas>
    </Canvas>
    
</Window>
